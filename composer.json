    "keywords": ["akeneo", "currency", "conversion", "rate"],
    "type": "library",
    "minimum-stability": "stable",
    "license": "OSL-3.0",
    "version": "5.0",
    "authors": [
      {
        "name": "Krish Technolabs",
        "email": "krishan.kant@krishtechnolabs.com"
      }
    ],
    "autoload": {
